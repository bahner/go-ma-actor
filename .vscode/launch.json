    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch actor forced generation",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/actor/",
            "console": "integratedTerminal",
            "args": [
                "--generate",
                "--force"
            ],
            "env": {
                "GOLOG_FILE": "debug.log",
                "GOLOG_LOG_LEVEL": "debug",
                "GOLOG_stdout": "false",
                "GOLOG_OUTPUT": "file"
            }
        },
        {
            "name": "Launch plain",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/actor/",
            "console": "integratedTerminal",
            "env": {
                "GOLOG_FILE": "/tmp/debug.log",
                "GOLOG_LOG_LEVEL": "debug",
                "GOLOG_stdout": "false",
                "GOLOG_OUTPUT": "file"
            }
        },
        {
            "name": "go-ma-actor generate",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/actor/",
            "console": "integratedTerminal",
            "env": {
                "GOLOG_FILE": "debug.log",
                "GOLOG_LOG_LEVEL": "debug",
                "GOLOG_stdout": "false",
                "GOLOG_OUTPUT": "file"
            },
            "args": [
                "--generate",
                "--nick",
                "FUBAR",
                "--force"
            ]
        },    
        {
            "name": "go-ma-actor-relay generate",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/relay/",
            "console": "integratedTerminal",
            "env": {
                "GOLOG_FILE": "debug.log",
                "GOLOG_LOG_LEVEL": "debug",
                "GOLOG_stdout": "false",
                "GOLOG_OUTPUT": "file"
            },
            "args": [
                "--generate",
            ]
        }    
    ]
}